{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "app/*": ["*"],
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "pages/*": ["src/pages/*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"],
      "vue$": ["node_modules/vue/dist/vue.runtime.esm-bundler.js"],
      "waves/components/*": ["node_modules/quasar-waves/src/components/*"],
      "waves/store/*": ["node_modules/quasar-waves/src/store/*"]
    }
  },
  "exclude": ["dist", ".quasar", "node_modules"]
}
